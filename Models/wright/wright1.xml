<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>wright.ac</path>

  <animation>
    <object-name>centre</object-name>
    <object-name>axe1</object-name>
    <object-name>axe2</object-name>
    <object-name>cache</object-name>
    <object-name>anneau</object-name>

    <object-name>moteur</object-name>

    <!-- transparent objects -->
  </animation>

  <model>
    <name>Bloc1</name>
    <path>Aircraft/Ford-Trimotor/Models/wright/blocpiston.xml</path>
  </model>

  <model>
    <name>Bloc2</name>
    <path>Aircraft/Ford-Trimotor/Models/wright/blocpiston.xml</path>
    <offsets>
      <roll-deg> 40 </roll-deg>
    </offsets>
  </model>

  <model>
    <name>Bloc3</name>
    <path>Aircraft/Ford-Trimotor/Models/wright/blocpiston.xml</path>
    <offsets>
      <roll-deg> 80 </roll-deg>
    </offsets>
  </model>

  <model>
    <name>Bloc4</name>
    <path>Aircraft/Ford-Trimotor/Models/wright/blocpiston.xml</path>
    <offsets>
      <roll-deg> 120 </roll-deg>
    </offsets>
  </model>

  <model>
    <name>Bloc5</name>
    <path>Aircraft/Ford-Trimotor/Models/wright/blocpiston.xml</path>
    <offsets>
      <roll-deg> 160 </roll-deg>
    </offsets>
  </model>

  <model>
    <name>Bloc6</name>
    <path>Aircraft/Ford-Trimotor/Models/wright/blocpiston.xml</path>
    <offsets>
      <roll-deg> 200 </roll-deg>
    </offsets>
  </model>

  <model>
    <name>Bloc7</name>
    <path>Aircraft/Ford-Trimotor/Models/wright/blocpiston.xml</path>
    <offsets>
      <roll-deg> 240 </roll-deg>
    </offsets>
  </model>

  <model>
    <name>Bloc8</name>
    <path>Aircraft/Ford-Trimotor/Models/wright/blocpiston.xml</path>
    <offsets>
      <roll-deg> 280 </roll-deg>
    </offsets>
  </model>

  <model>
    <name>Bloc9</name>
    <path>Aircraft/Ford-Trimotor/Models/wright/blocpiston.xml</path>
    <offsets>
      <roll-deg> 320 </roll-deg>
    </offsets>
  </model>

  <animation>
    <name>HighPoly</name>
    <object-name>centre</object-name>
    <object-name>axe1</object-name>
    <object-name>axe2</object-name>
    <object-name>cache</object-name>
    <object-name>anneau</object-name>
    <object-name>Bloc1</object-name>
    <object-name>Bloc2</object-name>
    <object-name>Bloc3</object-name>
    <object-name>Bloc4</object-name>
    <object-name>Bloc5</object-name>
    <object-name>Bloc6</object-name>
    <object-name>Bloc7</object-name>
    <object-name>Bloc8</object-name>
    <object-name>Bloc9</object-name>
  </animation>

  <animation>
    <name>LowPoly</name>
    <object-name>moteur</object-name>
    <object-name>centre</object-name>
    <object-name>axe1</object-name>
    <object-name>axe2</object-name>
    <object-name>cache</object-name>
    <object-name>anneau</object-name>
  </animation>

  <model>
    <path>Aircraft/Ford-Trimotor/Models/wright/Propeller/propellerC.xml</path>
  </model>

  <animation>
    <type>range</type>
    <object-name>LowPoly</object-name>
    <min-property>sim/rendering/static-lod/detailed</min-property>
  </animation>

  <animation>
    <type>range</type>
    <object-name>HighPoly</object-name>
    <min-m>0</min-m>
    <max-property>sim/rendering/static-lod/detailed</max-property>
  </animation>

</PropertyList>

